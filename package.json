    "ssb-box2": "^3.0.1",
    "ssb-crut": "^4.6.1",
    "ssb-meta-feeds": "~0.30.0",
    "ssb-private-group-keys": "^1.0.0",
    "ssb-uri2": "^2.4.0"
  },
  "devDependencies": {
    "c8": "^7.11.3",
    "husky": "4.3",
    "prettier": "^2.6.2",
